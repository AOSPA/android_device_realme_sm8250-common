    <integer-array name="config_udfps_sensor_props" translatable="false" >
        <item>550</item>  <!-- <item>sensorLocationX</item> -->
        <item>2202</item> <!-- <item>sensorLocationY</item> -->
        <item>104</item>   <!-- <item>sensorRadius</item> -->
    </integer-array>

